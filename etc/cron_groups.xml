<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/cron_groups.xsd">
    <group id="cmscheduler_crons">
        <schedule_generate_every>10</schedule_generate_every>
        <schedule_ahead_for>5</schedule_ahead_for>
        <schedule_lifetime>20</schedule_lifetime>
        <history_cleanup_every>15</history_cleanup_every>
        <history_success_lifetime>10080</history_success_lifetime>
        <history_failure_lifetime>10080</history_failure_lifetime>
        <use_separate_process>1</use_separate_process>
    </group>
</config>
